  /* 头像部分 */
.head-box{
    height: 170rpx;
    background-color: darksalmon;
    text-align: center;
    line-height: 170rpx;
    display: flex;
    justify-content:center;
    flex-direction: column;
    align-items: center;
    border-radius: 20px 20px 0px 0px;
}
.avatar-wrapper{
    height: 150rpx;
    border-radius: 40rpx;
    margin: 0;
    padding: 0;
}
.avatar{
    display: inline-block;
    width: 100%;
    height: 100%;
}
.cell-panel-demo{
    display: block;
    margin-top: 5rpx;
}

  

  
