/* pages/food/food.wxss */

.food{
  display: grid;
  margin: 72rpx 16rpx;
}
.food > navigator{
  display: flex;
  height: 240rpx;
  border-radius: 30rpx;
  background-color: #fbfbfb;
  margin-bottom: 16rpx;
  padding: 24rpx;
  box-sizing: border-box;
}
.food > navigator > image{
  height: 100%;
  margin-right: 24rpx;
}
.card-info{
  height: 100%;
  flex: 1;
}
/* 标题部分 */
.card-info >text:nth-of-type(1){
  font-size: 1.2em;
  font-weight: 600;
}

